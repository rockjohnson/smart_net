#ifndef __SERVICE_H__
#define __SERVICE_H__

namespace nm_smartnet {

class IService
{
public:
	IService();
	virtual ~IService();

public:
	virtual int32_t start() = 0;
	virtual int32_t stop() = 0;


};

}

#endif /* SERVICE_H_ */
